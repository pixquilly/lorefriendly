{
    "name": "backend",
    "version": "1.0.0",
    "description": "backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "backend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@types/express": "^4.17.1",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.9.1",
        "mysql": "^2.18.1",
        "mysql2": "^3.12.0",
        "nodemon": "^3.1.9",
        "rotating-file-stream": "^1.4.6",
        "sequelize": "^6.37.6",
        "ts-node-dev": "^1.0.0-pre.43",
        "typescript": "^3.6.3"
    },
    "devDependencies": {
        "@types/node": "^22.13.9"
    }
